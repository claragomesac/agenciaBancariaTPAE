/**
 * Classe que representa um cliente.
 */
public class Cliente {
    private String nome;
 
    /**
     * Construtor da classe Cliente.
     * @param nome Nome do cliente.
    */
    public Cliente(String nome) {
        this.nome = nome;
     }

    /**
     * Obt√©m o nome do cliente.
     * @return Nome do cliente.
     */
    public String getNome() {
        return nome;
    }
   
}

